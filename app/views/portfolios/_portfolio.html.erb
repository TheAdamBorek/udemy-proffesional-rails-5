<p><%= link_to portfolio.title, portfolio_path(portfolio) %>: <%= portfolio.id %></p>
<p><%= portfolio.subtitle %></p>
<%= link_to "Edit", edit_portfolio_path(portfolio) %>
<p><%= portfolio.thumb_image %></p>
<%= image_tag portfolio.thumb_image unless portfolio.thumb_image.nil? %>
<p><%= link_to "Delete", portfolio_path(portfolio), method: :delete, data: {confirm: "Are you sure you want to delete #{portfolio.title}?"} %></p>